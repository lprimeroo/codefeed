<!DOCTYPE html>

  <head>
    <meta charset="UTF-8">
    <title>Welcome to Codefeed - Share and Solve</title>
    <link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="https://cdn.rawgit.com/google/code-prettify/master/loader/run_prettify.js"></script>
    <link rel="stylesheet" href="/loggedinhome.css">
    <style type="text/css">
      h1 {
        color : #fc4400;
      }
    </style>
  </head>

  <body>

    <nav class="nav">
   <ul class="nav__wrap">
      <li class="nav__list"><a href="/feed" class="nav__link">Home</a></li>
      <li class="nav__list"><a href="/add" class="nav__link">Add a problem</a></li>
      <li class="nav__list"><a href="/viewproblist" class="nav__link">View Solved</a></li>
      <li class="nav__list"><a href="/loggedinhome" class="nav__link">View Problems</a></li>
      <li class="nav__list"><a href="/showprofile" class="nav__link">Profile</a></li>
      <li class="nav__list"><a href="/rankings" class="nav__link">Rankings</a></li>
      <li class="nav__list"><a href="/logout" class="nav__link">Logout</a></li>
      <li class="nav__list"><a href="/settersolutions" class="nav__link">Your Problems</a></li>

   </ul>
</nav>   

<main class="content">
    <% documents.forEach(function(documentObject) { %>
    <h1><%= documentObject.problem_name %></h1>
    <h2>ID:<%= documentObject.problemid %></h2>
    <p><%= documentObject.problem_statement %></p>
    <h2>Input</h2>
    <pre class="prettyprint linenums:1"><%= documentObject.problem_input %></pre><br />
    <h2>Output</h2>
    <pre class="prettyprint linenums:1"><%= documentObject.problem_output %></pre><br />
    <h2>Date Added</h2>
    <p><%= documentObject.date_added %></p>
    <a href="/solve/<%= documentObject.problemid %>"><button id="submit">Submit Solution</button></a><br /><br />
    <% }) %>
</main>
    
    <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>  
    <script type="text/javascript" src="/loggedinhome.js"></script>  
  </body>
</html>
 

