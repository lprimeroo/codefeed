<main class="content">
<div id="twitterwidget">
    <% documents.forEach(function(documentObject) {  %>
      <%
      var availableemail;
      if(documentObject.facebook.email==null)
          availableemail=documentObject.local.email
      else
          availableemail=documentObject.facebook.email
        %>
        <div id="userprofile">
          <img></img>
          <h2><%= documentObject.name %><a><%= documentObject.username %></a><a><%= availableemail %></a><a><%= documentObject.date_joined %></a></h2>
        </div>
    <% }) %>

    <% var count=0 ;
    documents2.forEach(function(documentObject2) { %>
    <% count++  }) %>
    <div id="info">
      <div><span></span></div>
      <div><%= count %><span>Total Problems Solved</span></div>
    </div>
</div>

</main>
    
    <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>  
    <script type="text/javascript" src="/loggedinhome.js"></script>  

  </body>
